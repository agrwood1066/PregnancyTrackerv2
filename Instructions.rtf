// Example shopping list service\
export const ShoppingListService = \{\
  async getItems(userId: string) \{\
    const \{ data, error \} = await supabase\
      .from('shopping_lists')\
      .select('*')\
      .eq('user_id', userId)\
      .order('created_at', \{ ascending: false \});\
      \
    if (error) throw error;\
    return data;\
  \},\
  \
  async addItem(userId: string, item: \{ item_name: string, price?: number, price_source?: string, is_starred?: boolean \}) \{\
    const \{ data, error \} = await supabase\
      .from('shopping_lists')\
      .insert(\{\
        user_id: userId,\
        ...item\
      \})\
      .single();\
      \
    if (error) throw error;\
    return data;\
  \},\
  \
