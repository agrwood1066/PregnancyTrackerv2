\f0\b\fs32 \cf0 3. Authentication Implementation\
\pard\pardeftab560\slleading20\partightenfactor0

\f1\b0\fs26 \cf0 // pages/api/auth/[...nextauth].js\
import NextAuth from 'next-auth';\
import SupabaseAuth from 'next-auth/providers/supabaseauth\'92;\
import \{ createClient \} from '@supabase/supabase-js';\
\
// Initialize Supabase client\
const supabase = createClient(\
  process.env.NEXT_PUBLIC_SUPABASE_URL,\
  process.env.SUPABASE_SERVICE_ROLE_KEY\
);\
\
export default NextAuth(\{\
  providers: [\
    ReplitProvider(\{\
      clientId: process.env.REPLIT_CLIENT_ID,\
      clientSecret: process.env.REPLIT_CLIENT_SECRET,\
    \}),\
  ],\
  callbacks: \{\
    async signIn(\{ user, account \}) \{\
      if (!user.email) return false;\
      \
      // Check if user exists in Supabase\
      const \{ data, error \} = await supabase\
        .from('users')\
        .select('*')\
        .eq('replit_id', user.id)\
        .single();\
      \
      // If user doesn't exist, create them\
      if (error && error.code === 'PGRST116') \{\
        const \{ data: newUser, error: createError \} = await supabase\
          .from('users')\
          .insert(\{\
            replit_id: user.id,\
            email: user.email,\
            display_name: user.name || user.email.split('@')[0],\
          \})\
          .single();\
          \
        if (createError) return false;\
      \}\
      \
      return true;\
    \},\
    async session(\{ session, token \}) \{\
      // Add Supabase user data to session\
      if (session?.user) \{\
        const \{ data: userData \} = await supabase\
          .from('users')\
          .select('*')\
          .eq('replit_id', token.sub)\
          .single();\
          \
        if (userData) \{\
          session.user.id = userData.id;\
          session.user.household_id = userData.household_id;\
        \}\
      \}\
      return session;\
    \},\
  \},\
\});\
\pard\pardeftab560\sa40\partightenfactor0

