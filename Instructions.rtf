\f1\b0\fs26 \cf0 // pages/api/household/invite.ts\
import \{ supabase \} from '../../../services/supabaseService';\
\
export default async function handler(req, res) \{\
  const \{ userId, partnerEmail \} = req.body;\
  \
  // Create or get household ID\
  const \{ data: userData, error: userError \} = await supabase\
    .from('users')\
    .select('household_id')\
    .eq('id', userId)\
    .single();\
    \
  let householdId = userData?.household_id;\
  \
  if (!householdId) \{\
    // Create new household\
    const \{ data: household, error: householdError \} = await supabase\
      .rpc('create_household', \{ user_id: userId \});\
      \
    if (householdError) return res.status(500).json(\{ error: householdError.message \});\
    householdId = household;\
  \}\
  \
  // Check if partner exists\
  const \{ data: partnerData, error: partnerError \} = await supabase\
    .from('users')\
    .select('id')\
    .eq('email', partnerEmail)\
    .single();\
    \
  if (partnerError && partnerError.code !== 'PGRST116') \{\
    return res.status(500).json(\{ error: partnerError.message \});\
  \}\
  \
  if (partnerData) \{\
    // Update partner's household\
    const \{ error: updateError \} = await supabase\
      .from('users')\
      .update(\{ household_id: householdId \})\
      .eq('id', partnerData.id);\
      \
    if (updateError) return res.status(500).json(\{ error: updateError.message \});\
    \
    return res.status(200).json(\{ success: true \});\
  \} else \{\
    // Send invitation email (implementation needed)\
    // ...\
    \
    return res.status(200).json(\{ success: true, message: 'Invitation sent' \});\
  \}\
\}\
