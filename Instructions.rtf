\f0\b\fs32 \cf0 Database Schema\
Users Table\
\pard\pardeftab560\slleading20\partightenfactor0

\f1\b0\fs26 \cf0 CREATE TABLE users (\
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),\
  supabase_id TEXT UNIQUE NOT NULL,\
  email TEXT UNIQUE NOT NULL,\
  display_name TEXT,\
  household_id UUID,\
  created_at TIMESTAMP WITH TIME ZONE DEFAULT NOW(),\
  updated_at TIMESTAMP WITH TIME ZONE DEFAULT NOW()\
);\
\pard\pardeftab560\sa40\partightenfactor0

\f0\b\fs32 \cf0 Shopping Lists Table\
\pard\pardeftab560\slleading20\partightenfactor0

\f1\b0\fs26 \cf0 CREATE TABLE shopping_lists (\
  id UUID PRIMARY KEY DEFAULT uuid_generate_v4(),\
  user_id UUID REFERENCES users(id) NOT NULL,\
  item_name TEXT NOT NULL,\
  price DECIMAL(10,2),\
  price_source TEXT,\
  is_starred BOOLEAN DEFAULT FALSE,\
  is_purchased BOOLEAN DEFAULT FALSE,\
  created_at TIMESTAMP WITH TIME ZONE DEFAULT NOW(),\
  updated_at TIMESTAMP WITH TIME ZONE DEFAULT NOW()\
);\
\pard\pardeftab560\sa40\partightenfactor0

